        }
    </script>
    <link href="~/assets/Aadarsh/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />


    <link href="~/assets/Aadarsh/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/assets/Aadarsh/css/font-awesome.css" rel="stylesheet" />
    <!--pop-up-box-->

    <link href="~/assets/Aadarsh/css/popuo-box.css" rel="stylesheet" type="text/css" media="all" />
